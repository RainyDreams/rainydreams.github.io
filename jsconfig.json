{
"compilerOptions": {
  "module": "commonjs",
  "target": "ES6",
  "sourceMap": true,
  "outDir": "./dist",
  "baseUrl": "./",
  "paths": {
    "@/*": [
      "./src/*"
    ]
  },
  "types": [
    "node"
  ],
},
"exclude": [ // 要排除的文件
  "node_modules",
  "**/node_modules/*"
],
"include": [ // 包含的文件
  "src/*.js"
]
}